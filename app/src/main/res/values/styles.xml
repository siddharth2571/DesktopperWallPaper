<resources>


    <style name="Theme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="windowActionBar">false</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="MenuLabelsStyle">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:maxLines">2</item>
        <item name="android:ellipsize">middle</item>
    </style>

    <style name="MenuButtonsSmall">
        <item name="fab_size">normal</item>
        <item name="fab_colorNormal">@color/black_40</item>
        <item name="fab_colorPressed">@color/black_50</item>
        <item name="fab_colorRipple">@color/black_70</item>
    </style>

    <style name="Theme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <style name="etv_common_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center_vertical</item>
       <!-- <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/border_white_rounded</item>
        <item name="android:textCursorDrawable">@drawable/curser</item>-->
        <item name="android:cursorVisible">true</item>

        <item name="android:drawablePadding">3dp</item>
        <item name="android:textColorHint">@color/white_40</item>
        <item name="android:textSelectHandle">@color/red</item>
        <item name="android:textSize">20dp</item>
    </style>


</resources>
